{
  "expo": {
    "name": "Foodiez",
    "slug": "Foodiez",
    "description": "Foodiez is a food delivery app that allows you to order food from your favorite restaurants and have it delivered to your door.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/foodiez-logo.png",
    "userInterfaceStyle": "light",
    "scheme": ["com.auth.Foodiez", "fb48ae831f-4c83-499c-bded-4cc7e5201ca6"],
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": [
            "com.jgogoi.Foodiez"
          ],
          "enableGooglePay": false
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Foodiez to use your location."
        }
      ]
    ],
    "splash": {
      "image": "assets/foodiez-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#200763"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.auth.Foodiez",
      "bundleIdentifier": "com.auth.Foodiez"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.auth.Foodiez",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "48ae831f-4c83-499c-bded-4cc7e5201ca6"
      }
    }
  }
}
